{
  "tasks": {
    "build": "deno run -A npm:@react-router/dev@^7.13.0 build",
    "dev": "deno run -A npm:@react-router/dev@^7.13.0 dev",
    "start": "deno run -A npm:@react-router/serve@^7.13.0 ./build/server/index.js",
    "typegen": "deno run -A npm:@react-router/dev@^7.13.0 typegen",
    "typecheck": "deno task typegen && deno check .",
    "lint": "deno lint",
    "format": "deno fmt",
    "test": "deno test"
  },
  "imports": {
    "@deno/vite-plugin": "npm:@deno/vite-plugin@^1.0.6",
    "@std/yaml": "jsr:@std/yaml@^1.0.11",
    "@types/react": "npm:@types/react@^19.2.4",
    "@std/assert": "jsr:@std/assert@1",
    "@markdoc/markdoc": "npm:@markdoc/markdoc@^0.5.4",
    "@react-router/node": "npm:@react-router/node@^7.13.0",
    "@react-router/serve": "npm:@react-router/serve@^7.13.0",
    "@react-router/dev": "npm:@react-router/dev@^7.13.0",
    "@tailwindcss/vite": "npm:@tailwindcss/vite@^4.1.18",
    "isbot": "npm:isbot@^5.1.32",
    "react": "npm:react@^19.2.4",
    "react-dom": "npm:react-dom@^19.2.4",
    "react-router": "npm:react-router@^7.13.0",
    "tailwindcss": "npm:tailwindcss@^4.1.18",
    "vite": "npm:vite@^7.3.1",
    "zod": "npm:zod@^4.3.4"
  },
  "unstable": [
    "temporal"
  ],
  "nodeModulesDir": "auto",
  "include": [
    "**/*.ts",
    "**/*.tsx"
  ],
  "exclude": [
    ".react-router/**"
  ],
  "compilerOptions": {
    "lib": [
      "dom",
      "dom.iterable",
      "dom.asynciterable",
      "esnext",
      "deno.ns",
      "deno.unstable"
    ],
    "types": [
      "node",
      "vite/client",
      "react",
      "react-dom",
      "@types/react"
    ],
    "rootDirs": [
      ".",
      "./.react-router/types"
    ],
    "moduleResolution": "bundler",
    "jsx": "react-jsx",
    "jsxImportSource": "react",
    "verbatimModuleSyntax": true,
    "strict": true,
    "skipLibCheck": true,
    "noFallthroughCasesInSwitch": true,
    "noUncheckedIndexedAccess": true,
    "noImplicitOverride": true
  },
  "fmt": {
    "semiColons": false
  }
}